{
  "$schema": "./appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            }
          ]
        }
      }
    ]
  },
  "Umbraco": {
    "CMS": {
      "Content": {
        "MacroErrors": "Throw"
      },
      "Hosting": {
        "Debug": true
      },
      "RuntimeMinification": {
        "UseInMemoryCache": true,
        "CacheBuster": "Timestamp"
      },
      "Global": {
        "Smtp": {
          "From": "no-replay@idseefeld.de",
          "Host": "SMTP_SERVER",
          "Password": "PASSWORD",
          "Port": 465,
          "Username": "USERNAME",
          "SecureSocketOptions": "SslOnConnect"
        }        
      }
    }
  }
}
